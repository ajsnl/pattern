// Program to print a Alphabet Matrix Pattern
#include <stdio.h>

int main() {

    int n,i,j,space;
    char ch='A';
    printf("Enter the Limit\n");
    scanf("%d",&n);
    for(i=0;i<n;i++){
        for(j=0;j<=n;j++){
            printf("%c ",ch);
            ch++;
        }
        printf("\n");
    }

    return 0;
}